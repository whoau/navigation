{
  "manifest_version": 3,
  "name": "mytab",
  "version": "2.0.0",
  "description": "一个美观、可自定义的新标签页插件，类似WeTab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "topSites"
  ],
  "host_permissions": [
    "https://*.unsplash.com/*",
    "https://source.unsplash.com/*",
    "https://picsum.photos/*",
    "https://bing.biturl.top/*",
    "https://api.open-meteo.com/*",
    "https://api.sampleapis.com/*",
    "https://www.omdbapi.com/*",
    "https://openlibrary.org/*",
    "https://covers.openlibrary.org/*",
    "https://itunes.apple.com/*",
    "https://api.vvhan.com/*",
    "https://v1.hitokoto.cn/*",
    "https://v1.jinrishici.com/*",
    "https://api.openweathermap.org/*",
    "https://api.themoviedb.org/*",
    "https://ipapi.co/*",
    "http://ip-api.com/*",
    "https://api.douban.com/*",
    "https://music.163.com/*",
    "https://api.netease.com/*",
    "https://cdn.jsdelivr.net/*",
    "https://*.doubanio.com/*",
    "https://*.qq.com/*",
    "https://y.qq.com/*",
    "https://img1.doubanio.com/*",
    "https://img2.doubanio.com/*",
    "https://img3.doubanio.com/*",
    "https://*.douban.com/*"
  ],
  "icons": {
    "16": "icons/mytab-16.png",
    "32": "icons/mytab-32.png",
    "48": "icons/mytab-48.png",
    "128": "icons/mytab-128.png"
  }
}
