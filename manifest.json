{
  "manifest_version": 3,
  "name": "MyTab - 自定义新标签页",
  "version": "2.0.0",
  "description": "一个美观、可自定义的新标签页插件，类似WeTab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "topSites"
  ],
  "host_permissions": [
    "https://*.unsplash.com/*",
    "https://source.unsplash.com/*",
    "https://picsum.photos/*",
    "https://bing.biturl.top/*",
    "https://api.open-meteo.com/*",
    "https://api.sampleapis.com/*",
    "https://www.omdbapi.com/*",
    "https://openlibrary.org/*",
    "https://covers.openlibrary.org/*",
    "https://itunes.apple.com/*",
    "https://api.vvhan.com/*",
    "https://api.openweathermap.org/*",
    "https://api.themoviedb.org/*",
    "https://ipapi.co/*",
    "http://ip-api.com/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
